<launch>
	<!-- Launch the autonomy -->
	<include file="$(find rds_map_nav)/launch/core/sr_control.launch">
		<!-- Behavior is specified in these configuration files -->
		<arg name="conf_path_yaml"		value="$(find rds_path_follow)/conf/husky_slow.yaml"/>
		<arg name="conf_map_yaml" 		value="$(find rds_map_nav)/launch/ta_husky_default.yaml"/>
		<arg name="conf_map_io_yaml"	value="$(find rds_map_nav)/launch/ta_husky_default_io.yaml"/>
		<arg name="conf_com_yaml"		value="$(find rds_map_nav)/launch/ta_commander_default.yaml"/>
		
		<!-- Specify if debug output should appear in the terminal -->
		<arg name="output" 				value="screen"/>
		
		<!-- Prefix of topics generated by autonomy package -->
		<arg name="group_name"		value="/husky/rds"/>
		
		<!-- Coordinate frame of the mapping and also outputs generated by this system -->
		<arg name="frame_name"		value="/world"/>	
		
		<!-- Necessary topics to enable the autonomy -->
		<arg name="robot_cmd_vel" value="/X1/cmd_vel"/>	<!-- Command velocity to drive the robot -->
		<arg name="cloud0" value="/X1/points"/>		<!-- Input point cloud from sensor 0 -->

		<!-- <arg name="goals"	value="/robot0/rds/out4"/>	-->

		<arg name="tfr_enable" value="true"/> 
		<arg name="tfr_parent" value="world"/> 
		<arg name="tfr_child" value="X1/base_link"/> 
		
		<arg name="tf0_enable" value="true"/> 
		<arg name="tf0_parent" value="world"/> 
		<arg name="tf0_child" value="X1/base_link/front_laser"/> 
		
		<arg name="tracker_enable" value="true"/> 
			
	</include>
		
</launch>

